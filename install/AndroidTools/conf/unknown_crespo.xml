<?xml version="1.0" encoding="UTF-8"?>

<!--  Android unknown crespo configuration file   -->
<!--   Screen resolution 480x800   -->
<Android-config>
  <CanalPattern>
    <Pattern canal="keyboard" value="herring-keypad"/>
    <Pattern canal="touchscreen" value="mxt224_ts_input"/>
  </CanalPattern>
  <KeyMapping canal="keyboard">
    <Key name="POWER" code="116"/>
    <Key name="VOLUME_UP" code="115"/>
    <Key name="VOLUME_DOWN" code="114"/>
  </KeyMapping>
  <SoftKeyMapping>
    <Key name="HOME" avgX="425" avgY="750"/>
    <Key name="MENU" avgX="184" avgY="750"/>
    <Key name="SEARCH" avgX="302" avgY="750"/>
    <Key name="BACK" avgX="64" avgY="750"/>
  </SoftKeyMapping>
  <Touchscreen>
    <Pattern name="X" value="3 53 "/>
    <Pattern name="Y" value="3 54 "/>
    <Pattern name="ratioX" value="2.13"/>
    <Pattern name="ratioY" value="1.18"/>
    <Pattern name="up" value="3 48 0"/>
    <Threshold name="move" value="15"/>
    <Option name="sendMouseDownForMove" value="true"/>
    <Option name="useSmartTouchDetection" value="true" />
    <Option name="useMonkeyForPress" value="true"/>
  </Touchscreen>
</Android-config>
